{
	"report":"download_flow_v1",
	"topic":"download_flow",
	"skip_timestamp":true,
	"node_list":
	[
		{
			"event_seg":"peer_id"
		},
		{
		  "event_seg":"play_type",
		  "json_seg":"type",
		  "required":false
		}
	],
	"segments":
	[
		{
			"seg_name":"downloads",
			"split":true,
			"node_list":
			[
				{
					"event_seg":"url"
				},
                {
                  "event_seg":"file_id",
                  "required":false
                },
                {
                  "event_seg":"fsize",
                  "required":false
                },
				{
					"event_seg":"user_agent",
					"required":false
				}
			],
			"segments":
			[
				{
					"seg_name":"flow",
					"split":true,
					"node_list":
					[
						{
							"event_seg":"timestamp",
							"converter":"TIMESTAMP_CURRENT"
						},
						{
							"event_seg":"duration",
							"converter":"TIMESTAMP_MILLISECOND"
						},
						{
							"event_seg":"p2p_download",
							"json_seg":"p2pDown"
						},
						{
							"event_seg":"cdn_download",
							"json_seg":"httpDown"
						}
					]
				}
			]
		}
	]
}